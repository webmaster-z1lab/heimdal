{
    "name": "optimus/heimdal",
    "autoload": {
        "psr-4": {
            "Optimus\\Heimdal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "require": {
        "laravel/framework": "^8.0",
        "ext-json": "*",
        "asm89/stack-cors": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "~v6.0",
        "phpunit/phpunit": "^9.3",
        "bugsnag/bugsnag-laravel": "^2.20",
        "php-coveralls/php-coveralls": "^2.1",
        "laravel/passport": "^10.1"
    },
    "suggest": {
        "asm89/stack-cors": "Needed to add CORS headers if add_cors_headers configuration is true",
        "sentry/sentry": "Needed to report exceptions to Sentry.io",
        "rollbar/rollbar": "Needed to report exceptions to Rollbar.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
